
@{
    ViewBag.Title = "LogList";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayout.cshtml";
}

@model List<DTO.LogDTO>

<section class="content-header">
    <h1>Log List</h1>
    <ol class="breadcrumb float-sm-left">
        <li class="breadcrumb-item"><a>Admin</a></li>
        <li class="breadcrumb-item"><a>Log</a></li>
        <li class="breadcrumb-item active"><a>Log List</a></li>
    </ol>
</section>

<br />

<div class="card-footer">
    <button class="btn btn-primary" onclick="window.location.href='/Admin/Post/PostList'">Post List</button>
</div>

<div class="card">

    <!-- table -->
    <div class="card-body">
        <table id="example1" class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>User Name</th>
                    <th>Table Name</th>
                    <th>Table ID</th>
                    <th>Process Name</th>
                    <th>Process Date</th>
                    <th>IP Address</th>
                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
                {

                    <tr id="a_@item.ID">

                        <td>@item.UserName</td>

                        <td>@item.TableName</td>

                        <td>@item.TableID</td>

                        <td>@item.ProcessName</td>

                        <td>@item.ProcessDate</td>

                        <td>@item.IpAddress</td>

                    </tr>

                }
            </tbody>
                <tfoot>
                    <tr>
                        <th>User Name</th>
                        <th>Table Name</th>
                        <th>Table ID</th>
                        <th>Process Name</th>
                        <th>Process Date</th>
                        <th>IP Address</th>
                    </tr>
                </tfoot>
            </table>
    </div>
</div>

